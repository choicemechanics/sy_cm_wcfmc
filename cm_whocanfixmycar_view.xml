<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="view_whocanfixmycar_form">
            <field name="name">WhoCanFixMyCar</field>
            <field name="model">cm.whocanfixmycar</field>
            <field name="arch" type="xml">
            <form string="WhoCanFixMyCar">
                <sheet>
                    <group col="4">
                        <field name="wcfmc_username" required="1"/>
                        <field name="wcfmc_password" required="1" password="True"/>
                        <field name="runscope_auth_token"/>
                    </group>
                </sheet>
            </form>
            </field>
        </record>
        <record model="ir.ui.view" id="view_whocanfixmycar_tree">
            <field name="name">WhoCanFixMyCar</field>
            <field name="model">cm.whocanfixmycar</field>
            <field name="arch" type="xml">
            <tree string="WhoCanFixMyCar">
                <field name="wcfmc_username" required="1"/>
                <field name="wcfmc_password" required="1" password="True"/>
                <field name="runscope_auth_token"/>
            </tree>
            </field>
        </record>

        <record id="act_view_whocanfixmycar" model="ir.actions.act_window">
            <field name="res_model">cm.whocanfixmycar</field>
            <field name="view_type">form</field>
            <field name="name">WhoCanFixMyCar</field>
            <field name="view_mode">tree,form</field>
        </record>

        
        <menuitem id="menu_whocanfixmycar" parent="base.menu_config" name="WhoCanFixMyCar" action="act_view_whocanfixmycar"/>
        
    </data>
</openerp>